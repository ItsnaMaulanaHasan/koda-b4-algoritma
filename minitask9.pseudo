DECLARE Nilai : DOUBLE

INPUT Nilai

IF (Nilai >= 90 && Nilai <= 100) THEN
    OUTPUT "A"
ELSEIF (Nilai < 90 && Nilai >= 75) THEN
    OUTPUT "B"
ELSEIF (Nilai < 75 && Nilai >= 60) THEN
    OUTPUT "C"
ELSEIF (Nilai < 60 && Nilai >= 40) THEN
    OUTPUT "D"
ELSEIF (Nilai < 40 && Nilai >= 20) THEN
    OUTPUT "E"
ELSEIF (Nilai < 20 && Nilai >= 0) THEN
    OUTPUT "F"
ELSE
    OUTPUT "ERROR: Nilai Diluar Jangkauan"
ENDIF